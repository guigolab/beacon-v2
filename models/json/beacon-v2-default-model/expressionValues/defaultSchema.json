{
    "$comment": "version: ga4gh-beacon-expression-value-v2.1.0",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "additionalProperties": true,
    "description": "Schema for an expression value entry. It corresponds of a cell of a matrix",
    "properties": {
        "id":{
            "type": "string",
            "description": "identifier needed for internal usage"
        },
        "expressionValue": {
            "description": "expression value number.",
            "type": "number"
        },
        "biosampleId": {
            "type": "string"
        },
        "featureId": {
            "type": "string"
        },
        "unit":{
            "type":"string",
            "enum":["TPM","RPM","RPKM","FPKM","raw"]
        }
    },
    "required": [
        "expressionValue",
        "unit",
        "biosampleId",
        "featureId"
    ],
    "title": "Expression Value",
    "type": "object"
}