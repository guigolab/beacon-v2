{
    "$comment": "version: ga4gh-beacon-feature-v2.1.0",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "additionalProperties": true,
    "description": "Schema for a feature value. It can be any biological sequence mapped to a genome, gene, transcript, genomic location",
    "properties": {
        "id": {
            "type": "string",
            "description": "Sequence feature identifier"
        },
        "location": {
            "$ref": "https://raw.githubusercontent.com/ga4gh/vrs/main/schema/vrs.json#/definitions/Location",
            "description": "Use of VRS location"
        },
        "seqOntologyRef": {
            "type": "string",
            "description": "Sequence Ontology Reference, will change in favor of VRS type"
        },
        "bioPathwayID":{
            "type":"string",
            "description": "A GO or KEGG identifier. Example: GO:0008150 or KEGG:hsa00010"
        },
        "identifiers": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "matrices": {
            "type": "array",
            "description": "Related matrices ",
            "items": {
                "type": "string"
            }
        }
    },
    "required": [
        "id",
        "location"
    ],
    "title": "Sequence feature object",
    "type": "object"
}